{
  "$schema": "https://github.com/dotnet/Nerdbank.GitVersioning/raw/main/src/NerdBank.GitVersioning/version.schema.json",
  "version": "2.0-pre",
  "buildNumberOffset": 3,
  "assemblyVersion": {
    "precision": "revision"
  },
  "publicReleaseRefSpec": [
    "^refs/heads/master$",
    "^refs/heads/develop$",
    "^refs/heads/v\\d+\\.\\d+$"
  ]
}
